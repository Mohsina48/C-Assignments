#include <stdio.h>
int main()
int amount, hundred_notes, fifty_notes, ten_notes;
printf("Enter the amount to withdraw (in rupees)"); scanf("%d", &amount);
hundred_notes = amount / 100;
amount = amount % 100;
fifty_notes = amount / 50; amount = amount % 50;
ten notes amount / 10;
amount = amount % 10;
printf("Currency notes to be given: \n");
printf("100% = %d\n", hundred_notes);
printf("50% = %d\n", fifty_notes);
printf("105 = %d", ten_notes);
}
